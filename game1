<script>
var max=10,min=1;
var x=Math.floor(Math.random()*(max-min)+min);
function find(x,bn){
if(x==bn){
alert("Ти виграв(ла)")
}
else{
alert("Ти програв(ла)")
}
}
function q(){
find(x,1)
}
function w(){
find(x,2)
}
function e(){
find(x,3)
}
function r(){
find(x,4)
}
function t(){
find(x,5)
}
function y(){
find(x,6)
}
function u(){
find(x,7)
}
function i(){
find(x,8)
}
function o(){
find(x,9)
}
console.log(x)
</script>
